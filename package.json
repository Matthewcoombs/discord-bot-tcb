{
  "name": "discord-bot",
  "version": "13.14.0",
  "description": "discord bot",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "start": "tsc && node dist/index.js",
    "start-dev": "tsc && ts-node index.ts",
    "build": "pnpm install && tsc",
    "build-prod": "/root/.local/share/pnpm/pnpm install --prod --ignore-scripts && tsc --skipLibCheck",
    "dev": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"pnpm start-dev\"",
    "update-commands": "node ./scripts/deploy-commands.js",
    "delete-commands": "node ./scripts/delete-commands.js",
    "update-commands-dev": "ts-node ./scripts/deploy-commands.js",
    "delete-commands-dev": "ts-node ./scripts/delete-commands.js",
    "reload": "pnpm delete-commands-dev && pnpm update-commands-dev"
  },
  "author": "matthew c",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "axios": "^1.12.2",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "openai": "^6.7.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/pg": "^8.15.4",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.7.3",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
